{
  "name": "strapixi-social-cms",
  "version": "1.0.0",
  "description": "Enterprise-grade Strapi v5 Social Media Content Management System",
  "private": true,
  "scripts": {
    "develop": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "strapi": "strapi",
    "test": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "seed": "node scripts/seed.js",
    "migrate": "node scripts/migrate.js"
  },
  "dependencies": {
    "@strapi/strapi": "^5.4.2",
    "@strapi/plugin-users-permissions": "^5.4.2",
    "@strapi/plugin-i18n": "^5.4.2",
    "@strapi/plugin-cloud": "^5.4.2",
    "@strapi/plugin-graphql": "^5.4.2",
    "@strapi/plugin-sentry": "^5.4.2",
    "better-sqlite3": "^11.7.0",
    "pg": "^8.13.1",
    "mysql2": "^3.11.5",
    "redis": "^4.7.0",
    "ioredis": "^5.4.2",
    "axios": "^1.7.9",
    "node-cron": "^3.0.3",
    "dayjs": "^1.11.13",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "uuid": "^11.0.3",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "dotenv": "^16.4.7",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/uuid": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11"
  },
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  },
  "author": "StrapiXi Team",
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
